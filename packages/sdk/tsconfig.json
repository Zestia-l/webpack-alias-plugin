{
    "compilerOptions": {
      "baseUrl": "./",              
      "paths": {                   
        "/*": ["src/*"],
        "components/*": ["src/components/*"],
        "pages/*": ["src/pages/*"]
      },
      "resolveJsonModule": true,
      "jsx": "react-jsx",
      "jsxImportSource": "react",       
      "lib": ["dom", "esnext"], 
      "moduleResolution": "node" ,
      "esModuleInterop": true,
      "composite": true
    },
    "extends": "../../tsconfig.json"
  }